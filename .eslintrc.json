{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "jsdoc",
        "prettier"
    ],
    "extends": [
        "google",
        "plugin:jsdoc/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "valid-jsdoc": "error",
        "new-cap": "off"
    },
    "settings": {
        "jsdoc": {
            "tagNamePreference": {
                "returns": "return"
            }
        }
    }
}
